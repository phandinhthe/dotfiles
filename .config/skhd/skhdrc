# Changing window focus


cmd + shift - h : yabai -m window --focus west
cmd + shift - j : yabai -m window --focus south
cmd + shift - k : yabai -m window --focus north
cmd + shift - l : yabai -m window --focus east


#Swap window.
cmd + shift - f : yabai -m window --swap east
cmd + shift - e : yabai -m window --swap north
cmd + shift - d : yabai -m window --swap south
cmd + shift - s : yabai -m window --swap west

# Toogle window float
shift + alt - t : yabai -m window --toggle float --grid 4:4:1:1:2:2

# Maximize window
shift + alt - f : yabai -m window --toggle zoom-fullscreen
